<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<fx:root type="javafx.scene.layout.HBox" xmlns:fx="http://javafx.com/fxml"
         styleClass="box"
         stylesheets="@player.css">
    <Button fx:id="rewind" styleClass="player-button" onAction="#rewind"/>
    <Button fx:id="play" styleClass="player-button" onAction="#play"/>
    <Button fx:id="forward" styleClass="player-button" onAction="#forward"/>
</fx:root>
